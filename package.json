{
	"name": "simonscholz",
	"version": "0.1.0",
	"description": "A simple starter to get up and developing quickly with Gatsby and TypeScript",
	"private": true,
	"author": "Simon Scholz <opensource.simon@gmail.com>",
	"homepage": "https://simonscholz.github.io/",
	"license": "MIT",
	"keywords": [
		"gatsby",
		"typescript",
		"emotion"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/SimonScholz/simonscholz.github.io.git"
	},
	"scripts": {
		"start": "npm run dev",
		"dev": "gatsby develop --host 0.0.0.0",
		"build": "gatsby build",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"gen": "apollo codegen:generate src/types/graphql.ts --endpoint=http://0.0.0.0:8000/___graphql --outputFlat --target=typescript --tagName=graphql --includes='src/**/*.{ts,tsx}' --no-addTypename --watch",
		"format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
		"lint": "tsc --noEmit && eslint . --ext .js --ext .ts --ext .tsx"
	},
	"dependencies": {
		"@emotion/core": "^11.0.0",
		"@emotion/react": "^11.4.1",
		"dotenv": "^10.0.0",
		"gatsby": "^4.3.0",
		"gatsby-image": "^3.11.0",
		"gatsby-plugin-canonical-urls": "^4.3.0",
		"gatsby-plugin-emotion": "^6.13.0",
		"gatsby-plugin-feed": "^3.14.0",
		"gatsby-plugin-gdpr-cookies": "^2.0.6",
		"gatsby-plugin-google-analytics": "^3.13.0",
		"gatsby-plugin-image": "^1.13.0",
		"gatsby-plugin-layout": "^2.13.0",
		"gatsby-plugin-manifest": "^3.13.0",
		"gatsby-plugin-netlify": "^3.14.0",
		"gatsby-plugin-nprogress": "^3.13.0",
		"gatsby-plugin-offline": "^4.13.0",
		"gatsby-plugin-postcss": "^4.13.0",
		"gatsby-plugin-purgecss": "^6.0.2",
		"gatsby-plugin-react-helmet": "^4.13.0",
		"gatsby-plugin-sharp": "^3.13.0",
		"gatsby-plugin-sitemap": "^4.9.0",
		"gatsby-plugin-typescript": "^3.13.0",
		"gatsby-plugin-webpack-size": "^2.0.1",
		"gatsby-remark-copy-linked-files": "^4.10.0",
		"gatsby-remark-images": "^5.10.0",
		"gatsby-remark-prismjs": "^5.10.0",
		"gatsby-remark-responsive-iframe": "^4.10.0",
		"gatsby-remark-smartypants": "^4.10.0",
		"gatsby-source-filesystem": "^3.13.0",
		"gatsby-transformer-remark": "^4.10.0",
		"gatsby-transformer-sharp": "^3.13.0",
		"postcss": "^8.4.5",
		"prismjs": "^1.25.0",
		"react": "^17.0.2",
		"react-cookie-consent": "^6.3.0",
		"react-dom": "^17.0.2",
		"react-ga": "^3.3.0",
		"react-helmet": "^6.1.0"
	},
	"devDependencies": {
		"@types/react": "^17.0.19",
		"@types/react-dom": "^17.0.9",
		"@types/react-helmet": "^6.1.4",
		"@typescript-eslint/eslint-plugin": "^4.30.0",
		"@typescript-eslint/parser": "^4.30.0",
		"apollo": "^2.33.6",
		"autoprefixer": "^10.3.3",
		"babel-eslint": "^10.0.3",
		"eslint": "^7.32.0",
		"eslint-config-react-app": "^6.0.0",
		"eslint-plugin-import": "^2.24.2",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-react": "^7.25.1",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^7.0.2",
		"postcss-import": "^14.0.2",
		"prettier": "^2.3.2",
		"pretty-quick": "^3.1.1",
		"tailwind-css-variables": "^3.0.1",
		"tailwindcss": "^2.2.9",
		"ts-node": "^10.2.1",
		"typescript": "^4.4.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
