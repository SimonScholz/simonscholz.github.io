{
	"name": "simonscholz",
	"version": "0.1.0",
	"description": "A simple starter to get up and developing quickly with Gatsby and TypeScript",
	"private": true,
	"author": "Simon Scholz <opensource.simon@gmail.com>",
	"homepage": "https://simonscholz.github.io/",
	"license": "MIT",
	"keywords": [
		"gatsby",
		"typescript",
		"emotion"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/SimonScholz/simonscholz.github.io.git"
	},
	"scripts": {
		"start": "npm run dev",
		"dev": "gatsby develop --host 0.0.0.0",
		"build": "gatsby build",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"gen": "apollo codegen:generate src/types/graphql.ts --endpoint=http://0.0.0.0:8000/___graphql --outputFlat --target=typescript --tagName=graphql --includes='src/**/*.{ts,tsx}' --no-addTypename --watch",
		"format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
		"lint": "tsc --noEmit && eslint . --ext .js --ext .ts --ext .tsx"
	},
	"dependencies": {
		"@emotion/core": "^11.0.0",
		"@emotion/react": "^11.8.2",
		"dotenv": "^16.0.0",
		"gatsby": "^4.10.0",
		"gatsby-image": "^3.11.0",
		"gatsby-plugin-canonical-urls": "^4.10.0",
		"gatsby-plugin-emotion": "^7.10.0",
		"gatsby-plugin-feed": "^4.10.0",
		"gatsby-plugin-gdpr-cookies": "^2.0.8",
		"gatsby-plugin-google-analytics": "^4.10.0",
		"gatsby-plugin-image": "^2.10.0",
		"gatsby-plugin-layout": "^3.10.0",
		"gatsby-plugin-manifest": "^4.10.0",
		"gatsby-plugin-netlify": "^4.1.0",
		"gatsby-plugin-nprogress": "^4.10.0",
		"gatsby-plugin-offline": "^5.10.0",
		"gatsby-plugin-postcss": "^5.10.0",
		"gatsby-plugin-purgecss": "^6.1.1",
		"gatsby-plugin-react-helmet": "^5.10.0",
		"gatsby-plugin-robots-txt": "^1.7.0",
		"gatsby-plugin-sharp": "^4.10.0",
		"gatsby-plugin-sitemap": "^5.10.0",
		"gatsby-plugin-typescript": "^4.10.0",
		"gatsby-remark-copy-linked-files": "^5.10.0",
		"gatsby-remark-images": "^6.10.0",
		"gatsby-remark-prismjs": "^6.10.0",
		"gatsby-remark-responsive-iframe": "^5.10.0",
		"gatsby-remark-smartypants": "^5.10.0",
		"gatsby-source-filesystem": "^4.10.0",
		"gatsby-transformer-remark": "^5.10.0",
		"gatsby-transformer-sharp": "^4.10.0",
		"postcss": "^8.4.12",
		"prismjs": "^1.27.0",
		"react": "^17.0.2",
		"react-cookie-consent": "^7.3.1",
		"react-dom": "^17.0.2",
		"react-ga": "^3.3.0",
		"react-helmet": "^6.1.0"
	},
	"devDependencies": {
		"@types/react": "^17.0.41",
		"@types/react-dom": "^17.0.13",
		"@types/react-helmet": "^6.1.5",
		"@typescript-eslint/eslint-plugin": "^5.15.0",
		"@typescript-eslint/parser": "^5.15.0",
		"apollo": "^2.33.9",
		"autoprefixer": "^10.4.4",
		"babel-eslint": "^10.0.3",
		"eslint": "^8.11.0",
		"eslint-config-react-app": "^7.0.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.3.0",
		"husky": "^7.0.4",
		"postcss-import": "^14.0.2",
		"prettier": "^2.6.0",
		"pretty-quick": "^3.1.3",
		"tailwind-css-variables": "^3.0.1",
		"tailwindcss": "^2.2.19",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
